BOOTLOADER:
PRINT "WANT TO INPUTSTACK?. Y-0 N-1"
READ
JMPZ INPUTSTACK
POP
PRINT "OKAY WHAT DO U WANT TO DO"
HALT




INPUTSTACK:
POP
PRINT "ENTER NO."
READ
PRINT "WANNA ADD MORE? Y-0 N-1"
READ
JMPZ INPUTSTACK
POP 
PRINT "WANNA PRINT? Y-0 N-1"
READ
JMPZ PRINTSTACK
PRINT "WHY DID U INPUT THEN BRUH?"
HALT



PRINTSTACK:
POP
TOP
POP
JMPZ END
PUSH 0
JMPZ PRINTSTACK
HALT


END:
PRINT "END"
HALT
